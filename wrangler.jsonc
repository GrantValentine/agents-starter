{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "my-chat-agent",
  "main": "src/server.ts",
  "compatibility_date": "2025-08-03",
  "compatibility_flags": ["nodejs_compat"],
  "ai": {
    "binding": "AI",
    "remote": true
  },
  "assets": {
    "directory": "public"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "Chat",
        "class_name": "Chat"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Chat"]
    }
  ],
  // ...existing config...
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "nov18_treasury_securities",
      "database_id": "fdc2dc16-e814-46d4-8509-87d165fac4bb"
    }
  ]
  
}
